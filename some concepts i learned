to initialise a 2-d vector
 vector<vector<int>> dp(A.size(),vector<int>(A.size(),INT_MAX));

to run a 2-d vector
for(auto& x:dp[dp.size()-1]){
            ans = min(ans,x);
    }

Packet making
int n = arr.size();
       int left = 0, right = 0;
       while(right<n)
       {
         while(right < n && arr[left] == arr[right])
             right++;
         
           cout<<right-left;
           left = right ;
       }

