#define ll long long
class Solution {
public:
    void reverseString(vector<char>& s) {
       ll l=s.size();
        ll k=0;
        ll temp;
        cout<<l;
        for(ll i=(l)-1;i>=l/2;i--)
        {
            temp=s[i];
            s[i]=s[k];
            s[k]=temp;
            k++;
        }
    }
};
